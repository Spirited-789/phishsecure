{
    "manifest_version": 3,
    "name": "PhishSecure",
    "version": "0.1.0",
    "description": "A browser extension for phishing protection.",
    "permissions": [
        "storage",
        "declarativeNetRequest",
        "alarms"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://raw.githubusercontent.com/"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "phishing_ruleset",
                "enabled": false,
                "path": "rulesets/phishing_rules.json"
            }
        ]
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "src/popup/popup.html"
    },
    "icons": {
        "16": "assets/icons/shield-16.png",
        "48": "assets/icons/shield-48.png",
        "128": "assets/icons/shield-128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "src/block_page/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}